vim Makefile.alphabet

CC=gcc
OBJ = alphabet.o

%.o: %.c 
	$(CC) -c -o $@ $<

alphabet.exe: $(OBJ)
	$(CC) -o $@ $^
  
  vim alphabet.c
  #include <stdio.h>
  int main()
  {
    printf("a:\a");
    printf("b:\b"); 
  /*printf("c:\c");*/
