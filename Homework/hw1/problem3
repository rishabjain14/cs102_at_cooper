  /*printf("i:\i");*/
  /*printf("j:\j");*/
  /*printf("k:\k");*/
  /*printf("l:\l");*/
  /*printf("m:\m");*/
    printf("n:\n"); 
    /*printf("o:\o");*/
    /*printf("p:\p");*/ 
    /*printf("q:\q");*/
    printf("r:\r"); 
    /*printf("s:\s");*/
    printf("t:\t"); 
    /*printf("u:\u");*/
    printf("v:\v");
    /*printf("w:\w");*/
    printf("x:\x");
    /*printf("y:\y");*/
    /*printf("z:\z");*/
    }
    
    /*(Terminal)*/
